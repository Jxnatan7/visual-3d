{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "web": {
        "bundler": "metro"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "http://localhost:3001"
      },
      "channel": "preview",
      "autoIncrement": true
    },
    "preview-ios": {
      "ios": {
        "buildConfiguration": "Release",
        "distribution": "internal"
      },
      "env": {},
      "channel": "preview-ios"
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      },
      "channel": "preview2"
    },
    "preview3": {
      "developmentClient": true,
      "channel": "preview3"
    },
    "preview4": {
      "distribution": "internal",
      "channel": "preview4"
    },
    "production": {
      "ios": {
        "buildConfiguration": "Release",
        "distribution": "store"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "env": {},
      "channel": "production",
      "autoIncrement": true
    }
  }
}
